#include "{{class.name}}_c.h"

class {{class.name}}
{
  var object_ : UnsafePointer<Void> = nil

  init(object:UnsafePointer<Void>)
  {
    object_ = object
  }

  deinit
  {
    {{class.name}}_dispose(object_);
  }
{% for method in class.methods %}
  func {{method.name}}({% for arg in method.arguments %}{%if not forloop.first %}, {% endif %}{{arg.name}}:{{arg.type}}{% endfor %})->{{method.return_type}}
  {
    return {{class.name}}_{{method.name}}(object_{% for arg in method.arguments %}, {{arg.name}}{% endfor %});
  }
{%endfor%}
}
{% for impl in impl_classes %}
class {{impl.name}} : {{class.name}}
{
public:
  {% for method in impl.constructors %}init({% for arg in method.arguments %}{% if not forloop.first %}, {% endif %}{{arg.type}} {{arg.name}}{% endfor %})
  {
    super.init({{class.name}}_{{impl.name}}_create({% for arg in method.arguments %}{% if not forloop.first %}, {% endif %}{{arg.name}}{% endfor %}))
  }{% endfor %}
}
{% endfor %}
